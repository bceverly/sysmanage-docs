{
  "repository": "sysmanage-server-windows",
  "description": "Windows MSI packages for SysManage Server",
  "updated": "2025-10-29T00:00:00Z",
  "platform": "Windows",
  "architectures": ["x64", "arm64"],
  "minimum_os": "Windows 10 1809 / Windows Server 2019",
  "package_format": "msi",
  "components": {
    "server": {
      "description": "SysManage Server for Windows",
      "architectures": ["x64", "arm64"],
      "install_location": "C:\\Program Files\\SysManage Server",
      "config_location": "C:\\ProgramData\\SysManage",
      "service_name": "SysManageServer"
    }
  },
  "dependencies": {
    "python": "3.12",
    "vcredist": "Visual C++ Redistributable (auto-installed)",
    "dotnet": ".NET Framework 4.5+ (for WiX)",
    "postgresql": ">=12.0 (recommended)",
    "nssm": "2.24 (bundled)"
  },
  "installation": {
    "method": "msiexec",
    "command": "msiexec /i sysmanage-{VERSION}-windows-{ARCH}.msi",
    "silent_install": "msiexec /i sysmanage-{VERSION}-windows-{ARCH}.msi /quiet /qn /norestart",
    "requires_admin": true
  },
  "service": {
    "name": "SysManageServer",
    "display_name": "SysManage Server",
    "startup_type": "Automatic",
    "recovery": "Restart on failure (3 attempts, 60s delay)",
    "log_rotation": "10MB per file, 5 files retained"
  }
}
